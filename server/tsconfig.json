{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"src/*"
			],
			"@shared/models/*": [
				"../models/*"
			],
		},
		"rootDirs": [
			"src"
		],
		"outDir": "dist",
		"lib": [
			"es2020"
		],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"types": [
			"node"
		]
	},
	"tsc-alias": {
		"resolveFullPaths": true,
		"verbose": false
	}
}